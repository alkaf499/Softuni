select_book = str(input())

counter = 0

while True:

    new_book = str(input())

    if select_book == new_book:

        print(f"You checked {counter} books and found it.")
        break

    if new_book == "No More Books":

        print(f"The book you search is not here!")
        print(f"You checked {counter} books.")
        break

    counter += 1

