# tyk moje da iska i flaot

goal = 10000

steps_total = 0

while True:

    steps = input()

    if steps == "Going home":

        last_steps = int(input())

        steps_total += last_steps

        if steps_total >= goal:

            diff = abs(steps_total - goal)
            print("Goal reached! Good job!")
            print(f"{diff} steps over the goal!")
            break

        else:

            diff = abs(steps_total - goal)
            print(f"{diff} more steps to reach goal.")
            break

    steps_total += int(steps)

    if steps_total >= goal:
        diff = abs(steps_total - goal)
        print("Goal reached! Good job!")
        print(f"{diff} steps over the goal!")
        break

