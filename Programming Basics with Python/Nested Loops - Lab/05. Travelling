for i in range(1, 1000):

  destination = str(input())

  if destination == "End":
    break
  
  price = float(input())

  Flag = False
  money = 0

  while money <= price or money > price:

    if money >= price:

      print(f"Going to {destination}!")
      break

    sum = input()

    money += float(sum)

    

        

  
    
  
    
    
    
    
