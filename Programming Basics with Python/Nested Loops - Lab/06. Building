number_of_floors = int(input())
apartments_per_floor = int(input())


for i in range(number_of_floors, 0, -1):

    for j in range (0, apartments_per_floor):

        if i == number_of_floors:

            print(f"L{i}{j}",end= " ")
            continue


        if i % 2 == 0:

            print(f"O{i}{j}",end= " ")

        else:

            print(f"A{i}{j}", end= " ")

    print("")

