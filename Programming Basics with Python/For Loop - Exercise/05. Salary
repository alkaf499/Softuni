number_of_tabs = int(input())
salary = int(input())

for i in range(1, number_of_tabs+1):
    a = str(input())
    i += 1

    if a == "Facebook":

        salary = salary - 150

    elif a == "Instagram":

        salary = salary - 100

    elif a == "Reddit":

        salary = salary - 50

if salary <= 0:

    print("You have lost your salary.")

else:

    print(salary)


