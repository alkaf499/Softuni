week_day = str(input())

if week_day == "Monday":
    print("12")

elif week_day  == "Tuesday":
    print("12")

elif week_day == "Wednesday":
    print("14")

elif week_day == "Thursday":
    print("14")

elif week_day == "Friday":
    print("12")

elif week_day == "Saturday":
    print("16")
    
elif week_day == "Sunday":
    print("16")
