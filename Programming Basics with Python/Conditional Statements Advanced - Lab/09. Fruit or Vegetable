product = str(input())

if product == "banana" or product == "lemon" or product == "grapes" or product == "apple" or product == "kiwi" or product == "cherry":

    print("fruit")

elif product == product == "tomato" or product == "cucumber" or product == "pepper" or product == "carrot":

    print("vegetable")

else:

    print("unknown")
