entered_number = float(input())

if entered_number == 0:

    print("zero")

elif entered_number > 0:

        if abs(entered_number) < 1:
            print("small positive")

        elif abs(entered_number) > 1000000:
            print("large positive")

        else:
            print("positive")

elif entered_number < 0:

    if abs(entered_number) < 1:
            print("small negative")

    elif abs(entered_number) > 1000000:
            print("large negative")

    else:

        print("negative")
